<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:m="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form">
	<f:SimpleForm id="SimpleFormId" editable="true" layout="ResponsiveGridLayout" title="" labelSpanXL="4" labelSpanL="4" labelSpanM="4"
		labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="2" columnsM="2"
		singleContainerFullSize="false" visible="{objectView>/editable}">
		<f:content>
			<core:Title text=""/>
			<m:Label text="{/#HeadType/Transtypecode/@sap:label}"/>
			<m:Text text="{TranstypecodeText}"/>
			<m:Label text="{/#HeadType/Retailstoreid/@sap:label}"/>
			<m:Text text="{RetailstoreidText}"/>
			<m:Label text="{/#HeadType/Auditbusinessdaydate/@sap:label}"/>
			<m:DatePicker id="Auditbusinessdaydate"
				value="{ 'path':'modelCreate>/Auditbusinessdaydate', 'type':'sap.ui.model.type.Date', 'formatOptions': { 'pattern': 'dd-MM-yyyy' } }"
				change="onChangeValue" class="sapUiSmallMarginBottom" editable="false"/>
			<m:Label text="{/#HeadType/beginFecha/@sap:label}"/>
			<m:Text text="{ 'path':'modelCreate>/beginFecha', 'type':'sap.ui.model.type.Date', 'formatOptions': { 'pattern': 'dd-MM-yyyy' } }"/>
			<m:Label text="{/#HeadType/Workstationid/@sap:label}"/>
			<m:Text text="{WorkstationidView}"/>
			<m:Label text="{/#HeadType/Transnumber/@sap:label}"/>
			<m:Text text="{TransnumberView}"/>
			<m:Label text="{/#HeadType/AltaManual/@sap:label}" visible="{AltaManual}"/>
			<m:Text text="{AltaManual}" visible="{AltaManual}"/>
			<m:Label text="{/#HeadType/ActHT/@sap:label}" visible="{AltaManual}"/>
			<m:CheckBox selected="{modelCreate>/ActHT}" visible="{AltaManual}" select="onSelectBajaEst"/>
			<m:Label text="{/#HeadType/BajaEst/@sap:label}" visible="{AltaManual}"/>
			<m:CheckBox selected="{modelCreate>/BajaEst}" visible="{AltaManual}" select="onSelectBajaEst"/>
			<!--CORE-->
			<core:Title text=""/>
			<m:Label text="{/#HeadType/Operatorid/@sap:label}"/>
			<m:Input id="Operatorid" value="{modelCreate>/Operatorid}" fieldGroupIds="fgDG" change="onChangeValue"
				maxLength="{= parseInt(${/#HeadType/Operatorid/maxLength}) }" required="true"/>
			<m:Label text="{/#HeadType/Origbusinessdate/@sap:label}"/>
			<m:DatePicker id="Origbusinessdate"
				required="{= ${modelCreate>/Transtypecode} === '102' || ${modelCreate>/Transtypecode} === '103' || ${modelCreate>/Transtypecode} === '104' ? true : false }"
				value="{ 'path':'modelCreate>/Origbusinessdate', 'type':'sap.ui.model.type.Date', 'formatOptions': { 'pattern': 'dd-MM-yyyy' } }"
				change="onChangeValue" class="sapUiSmallMarginBottom"/>
			<m:Label text="{/#HeadType/Origstoreid/@sap:label}"/>
			<m:Input id="Origstoreid" value="{modelCreate>/Origstoreid}" showSuggestion="true" showValueHelp="true"
				description="{modelCreate>/OrigstoreidText}" suggestionItems="{ path:'/zcds_audit_storeid_vh', templateShareable: true }"
				fieldGroupIds="fgDGMP" change="onChangeValue" valueHelpRequest="onValueHelpRequest"
				maxLength="{= parseInt(${/#HeadType/Origstoreid/maxLength}) }"
				required="{= ${modelCreate>/Transtypecode} === '102' || ${modelCreate>/Transtypecode} === '103' || ${modelCreate>/Transtypecode} === '104' ? true : false }">
				<m:suggestionItems >
					<core:Item text="{RetailstoreidText}" key="{Retailstoreid}"/>
				</m:suggestionItems>
			</m:Input>
			<m:Label text="{/#HeadType/Origwrkstid/@sap:label}"/>
			<m:Input id="Origwrkstid" value="{modelCreate>/Origwrkstid}" fieldGroupIds="fgDG" change="onChangeValue"
				maxLength="{= parseInt(${/#HeadType/Origwrkstid/maxLength}) }"
				required="{= ${modelCreate>/Transtypecode} === '102' || ${modelCreate>/Transtypecode} === '103' || ${modelCreate>/Transtypecode} === '104' ? true : false }"/>
			<m:Label text="{/#HeadType/Origtransnumber/@sap:label}"/>
			<m:Input id="Origtransnumber" value="{modelCreate>/Origtransnumber}" fieldGroupIds="fgDG" change="onChangeValue"
				maxLength="{= parseInt(${/#HeadType/Origtransnumber/maxLength}) }"
				required="{= ${modelCreate>/Transtypecode} === '102' || ${modelCreate>/Transtypecode} === '103' || ${modelCreate>/Transtypecode} === '104' ? true : false }"/>
			<m:Label text="{/#HeadType/Orgimptrans/@sap:label}"/>
			<m:Text
				text="{path:'modelCreate>/Orgimptrans', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"
				required="{= ${modelCreate>/Transtypecode} === '102' || ${modelCreate>/Transtypecode} === '103' || ${modelCreate>/Transtypecode} === '104' ? true : false }"/>
			<m:Label text=""/>
			<m:ObjectStatus id="objectStatError" visible="false" class="sapUiSmallMarginBottom" text="{i18n>errorOrgNoExist}" icon="sap-icon://error"
				state="Error"/>
			<!--CORE-->
			<core:Title text=""/>
			<m:Label text="{/#HeadType/Zzimptrans/@sap:label}"/>
			<m:Input id="Zzimptrans"
				value="{path:'modelCreate>/Zzimptrans', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"
				fieldGroupIds="fgDG" change="onChangeValue" required="true"/>
			<m:Label text="{/#HeadType/ImpTotaChange/@sap:label}"/>
			<m:Input id="Zzimptranscalc"
				value="{path:'modelCreate>/ImpTotaChange', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"
				fieldGroupIds="fgDG" change="onChangeValue" editable="false"/>
			<m:Label text="{/#HeadType/Zzformpagv/@sap:label}"/>
			<m:Text text="{zzFormpagvtext}({Zzformpagv})"/>
			<m:Label text="{/#HeadType/SenDescuentoV/@sap:label}"/>
			<m:Input id="SenDescuentoV" value="{modelCreate>/SenDescuento}" fieldGroupIds="fgDG" change="onChangeValue" maxLength="1" editable="false"/>
			<m:Label text="{/#HeadType/FechaCompromisoV/@sap:label}"/>
			<m:Text text="{ 'path':'modelCreate>/FechaCompromiso', 'type':'sap.ui.model.type.Date', 'formatOptions': { 'pattern': 'dd-MM-yyyy' } }"/>
			<m:Label text="{/#HeadType/FormaretirMerc/@sap:label}"/>
			<m:Text text="{modelCreate>/FormaretirMercV}"/>
		</f:content>
	</f:SimpleForm>
</core:FragmentDefinition>