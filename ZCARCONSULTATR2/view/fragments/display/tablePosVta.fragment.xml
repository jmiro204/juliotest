	<!--tableType="ResponsiveTable"-->
	<smartTable:SmartTable id="position" entitySet="Position" tableBindingPath="to_Position" smartFilterId="smartFilterBar" showRowCount="true"
		enableAutoBinding="true" tableType="Table" beforeRebindTable="onBeforeRebindTable">
		<smartTable:layoutData>
			<FlexItemData growFactor="1" baseSize="0%"/>
		</smartTable:layoutData>
		<Table id="gEmployeesTable" growing="true" growingScrollToLoad="true" growingThreshold="10">
			<columns>
				<Column width="100px" hAlign="Left">
					<customData>
						<core:CustomData key="p13nData"
							value='\{"columnKey": "ZzCalifArtic", "maxLength": "2","columnIndex":"9", "leadingProperty": "ZzCalifArtic"}'/>
					</customData>
					<Text text="{/#PositionType/ZzCalifArtic/@sap:label}"/>
				</Column>
				<Column width="100px" hAlign="Left">
					<customData>
						<core:CustomData key="p13nData" value='\{"columnKey": "Zzcampoc", "maxLength": "1","columnIndex":"10", "leadingProperty": "Zzcampoc"}'/>
					</customData>
					<Text text="{/#PositionType/Zzcampoc/@sap:label}"/>
				</Column>
				<Column width="100px" hAlign="Left">
					<customData>
						<core:CustomData key="p13nData" value='\{"columnKey": "Salesamount","columnIndex":"6", "leadingProperty": "Salesamount"}'/>
					</customData>
					<Text text="{/#PositionType/Salesamount/@sap:label}"/>
					<footer>
						<Text text="{path:'objectView>/Salesamount', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"/>
					</footer>
				</Column>
				<Column width="100px" hAlign="Left">
					<customData>
						<core:CustomData key="p13nData" value='\{"columnKey": "Zzlvimpneto","columnIndex":"7", "leadingProperty": "Zzlvimpneto"}'/>
					</customData>
					<Text text="{/#PositionType/Zzlvimpneto/@sap:label}"/>
					<footer>
						<Text text="{path:'objectView>/Zzlvimpneto', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"/>
					</footer>
				</Column>
				<Column width="100px" hAlign="Left">
					<customData>
						<core:CustomData key="p13nData" value='\{"columnKey": "Zzlvimpbruto","columnIndex":"8", "leadingProperty": "Zzlvimpbruto"}'/>
					</customData>
					<Text text="{/#PositionType/Zzlvimpbruto/@sap:label}"/>
					<footer>
						<Text text="{path:'objectView>/Zzlvimpbruto', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"/>
					</footer>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{= ${ZzCalifArtic} === '' ? '00' : ${ZzCalifArtic} }"/>
						<Text text="{= ${Zzcampoc} === '' ? '0' : ${Zzcampoc} }"/>
						<Text
							text="{path:'Salesamount', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"/>
						<Text
							text="{path:'Zzlvimpneto', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"/>
						<Text
							text="{path:'Zzlvimpbruto', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</smartTable:SmartTable>
</core:FragmentDefinition>