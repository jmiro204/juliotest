<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield">
	<smartForm:SmartForm editable="false">
		<smartForm:Group label="">
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/Transtypecode}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/Retailstoreid}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/AuditbusinessdaydateV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/beginFecha}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/WorkstationidView}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/TransnumberView}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement visible="{TrxanulV}">
				<smartField:SmartField value="{TrxanulV}"/>
			</smartForm:GroupElement>
		</smartForm:Group>
		<smartForm:Group label="" visible="{= ${to_HeadOrig/Transtypecode} !== '105' }">
			<smartForm:GroupElement label="{/#HeadType/Operatorid/@sap:label}">
				<!--<smartField:SmartField value="{to_HeadOrig/OperatoridV}"/>-->
				<Text  text="{ path: 'to_HeadOrig/OperatoridV', formatter: '.formatter.convertDeleteZero' }" editable="false"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/OrigbusinessdateV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/OrigstoreidV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/OrigwrkstidV}" width="111em"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/OrigtransnumberV}" width="111em"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/Orgimptrans}" width="111em"/>
			</smartForm:GroupElement>
		</smartForm:Group>
		<smartForm:Group label="" visible="{= ${to_HeadOrig/Transtypecode} === '105' }">
			<smartForm:GroupElement label="{/#HeadOrgType/OperatoridV/@sap:label}">
				<!--<smartField:SmartField value="{to_HeadOrig/Operatorid}"/>-->
				<Text  text="{ path: 'to_HeadOrig/OperatoridV', formatter: '.formatter.convertDeleteZero' }" editable="false"/>
			</smartForm:GroupElement>
		<!--	<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/beginFecha}"/>
			</smartForm:GroupElement>-->
			<smartForm:GroupElement label="{/#HeadType/vBusinessdaydate/@sap:label}">
				<smartField:SmartField value="{to_HeadOrig/VbusinessdaydateV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement label="{/#HeadType/vRetailstoreid/@sap:label}">
				<!--<smartField:SmartField value="{to_HeadOrig/VretailstoreidV}"/>-->
				<Text text="{ path: 'to_HeadOrig/VretailstoreidV', formatter: '.formatter.convertDeleteZero' }" editable="false" />
			</smartForm:GroupElement>
			<smartForm:GroupElement label="{/#HeadType/vWorkstationid/@sap:label}">
				<!--<smartField:SmartField value="{to_HeadOrig/VworkstationidV}"/>-->
					<Text  text="{ path: 'to_HeadOrig/VworkstationidV', formatter: '.formatter.convertTpv' }" editable="false" />
			</smartForm:GroupElement>
			<smartForm:GroupElement label="{/#HeadType/vTransnumber/@sap:label}">
				<!--<smartField:SmartField value="{to_HeadOrig/VtransnumberV}" width="1em"/>-->
				<Text text="{ path: 'to_HeadOrig/VtransnumberV', formatter: '.formatter.convertTpv' }" editable="false" />
			</smartForm:GroupElement>
		</smartForm:Group>
		<smartForm:Group label="">
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/ZzimptransV}" uomVisible="false"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/ZzformpagV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/SenDescuentoV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/FechaCompromisoV}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{to_HeadOrig/FormaretirMercV}"/>
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>