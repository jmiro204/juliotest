<mvc:View controllerName="app.inetum.zcarconsulta2.controller.Object" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.uxap" xmlns:m="sap.m"
	xmlns:smartForm="sap.ui.comp.smartform" xmlns:sap.ui.layout="sap.ui.layout" xmlns:smartTable="sap.ui.comp.smarttable"
	xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:smartField="sap.ui.comp.smartfield" height="100%" xmlns:table="sap.ui.table"
	xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:f="sap.ui.layout.form">
	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false" showFooter="{objectView>/editable}" busy="{objectView>/busy}">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading></expandedHeading>
				<snappedHeading></snappedHeading>
				<actions>
					<m:Button id="btnNavigateLog" text="Log Modificaciones" type="Emphasized" press="onNavigateLog"/>
					<m:Button id="btnEdit" text="Editar" type="Emphasized" press="onEdit" visible="{Update_mc}"
						enabled="{= ${objectView>/editable} ? false : true }"/>
					<!--<m:Button id="btnEdit" text="Editar" type="Emphasized" press="onEdit"/>-->
				</actions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent></headerContent>
		<sections>
			<ObjectPageSection titleUppercase="false" title="Datos de cabecera">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.display.form" type="XML"/>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.edit.form" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Comentario">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.display.formComment" type="XML"/>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.edit.formComment" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Errores de transaccíon">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.display.tableErrores" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Datos de medios de pago">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.display.tableMedPago" type="XML"/>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.edit.tableMedPago" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" title="Datos de posición venta">
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.display.tablePosVta" type="XML"/>
							<core:Fragment fragmentName="app.inetum.zcarconsulta2.view.fragments.edit.tablePosVta" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
		<footer>
			<m:OverflowToolbar>
				<m:ToolbarSpacer/>
				<m:Button type="Accept" text="Guardar" press="onSaveTransaction"/>
				<m:Button type="Reject" text="Cancelar" press="onCancelTransaction"/>
			</m:OverflowToolbar>
		</footer>
	</ObjectPageLayout>
</mvc:View>