<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table">
	<Dialog title="Mensajes" contentWidth="50%" contentHeight ="50%">
		<table:Table rows="{modelMessage>/}" selectionMode="None" width="100%">
			<table:columns>
				<table:Column width="100%">
					<Label text="Mensaje"/>
					<table:template>
						<Text text="{modelMessage>message}" wrapping="false"/>
					</table:template>
				</table:Column>
			</table:columns>
		</table:Table>
		<beginButton>
			<Button text="Ok" press="_onActionCancel"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>