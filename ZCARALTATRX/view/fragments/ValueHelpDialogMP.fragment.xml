<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="Product" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" afterClose=".onValueHelpAfterClose"
		supportRanges="false">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Tipomp" label="{/#zcar_cds_i_mediopago_vhType/Tipomp/@sap:label}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Tipomp"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Subtipmp" label="{/#zcar_cds_i_mediopago_vhType/Subtipmp/@sap:label}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Subtipmp"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Mediopagotext" label="{/#zcar_cds_i_mediopago_vhType/Mediopagotext/@sap:label}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Mediopagotext"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Mediopago" label="{/#zcar_cds_i_mediopago_vhType/Mediopago/@sap:label}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Mediopago"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Desgloce" label="{/#zcar_cds_i_mediopago_vhType/Desgloce/@sap:label}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Desgloce"/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>