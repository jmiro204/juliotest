					<m:Label text="Importe" textAlign="Center" width="100%"/>
				</multiLabels>
				<template>
					<m:Text text="{path:'modelVtaMov>TotImpor', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}">
						<m:customData>
							<core:CustomData key="title" value="{modelVtaMov>Group}" writeToDom="true"/>
						</m:customData>
					</m:Text>
				</template>
			</Column>
		</columns>
	</Table>
</core:FragmentDefinition>