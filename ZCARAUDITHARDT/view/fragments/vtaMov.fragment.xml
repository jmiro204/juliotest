<core:FragmentDefinition xmlns:m="sap.m" xmlns:core="sap.ui.core" xmlns="sap.ui.table">
	<Table id="tableVtaMov" ariaLabelledBy="title" selectionMode="None" enableColumnFreeze="true"
		rows="{ path : 'modelVtaMov>/' }"
		showColumnVisibilityMenu="true" firstVisibleRowChanged="onFirstChange">
		<columns>
			<Column width="18rem">
				<m:Label text="Tipo / Subtipo" textAlign="Center" width="100%"/>
				<template>
					<m:Text text="{modelVtaMov>Tiposubtipo} {modelVtaMov>Tiposubtipotext}">
						<m:customData>
							<core:CustomData key="title" value="{modelVtaMov>Group}" writeToDom="true"/>
						</m:customData>
					</m:Text>
				</template>
			</Column>
			<Column width="7.4rem" headerSpan="2,1">
				<multiLabels>
					<m:Label text="Remanente Inicial" textAlign="Center" width="100%"/>
					<m:Label text="Nº. Oper." textAlign="Center" width="100%"/>
				</multiLabels>
				<template>
					<m:Text text="{modelVtaMov>RemIniOper}">
						<m:customData>
							<core:CustomData key="title" value="{modelVtaMov>Group}" writeToDom="true"/>
						</m:customData>
					</m:Text>
				</template>
			</Column>
			<Column width="7.4rem" headerSpan="1">
				<multiLabels>
					<m:Label text="Remanente Inicial" textAlign="Center" width="100%"/>
					<m:Label text="Importe" textAlign="Center" width="100%"/>
				</multiLabels>
				<template>
					<m:Text text="{path:'modelVtaMov>RemIniImpor', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}">
						<m:customData>
							<core:CustomData key="title" value="{modelVtaNeta>Group}" writeToDom="true"/>
						</m:customData>
					</m:Text>
				</template>
			</Column>
			<Column width="7.4rem" headerSpan="2,1">
				<multiLabels>
					<m:Label text="Dotaciones" textAlign="Center" width="100%"/>
					<m:Label text="Nº. Oper." textAlign="Center" width="100%"/>
				</multiLabels>
				<template>
					<m:Text text="{modelVtaMov>DotOper}">
						<m:customData>
							<core:CustomData key="title" value="{modelVtaMov>Group}" writeToDom="true"/>
						</m:customData>
					</m:Text>
				</template>
			</Column>
			<Column width="7.4rem" headerSpan="1">
				<multiLabels>
					<m:Label text="Dotaciones" textAlign="Center" width="100%"/>
					<m:Label text="Importe" textAlign="Center" width="100%"/>
				</multiLabels>
				<template>
					<m:Text text="{path:'modelVtaMov>DotImpor', type:'sap.ui.model.type.Float', formatOptions: { maxIntegerDigits: 13, maxFractionDigits: 2, preserveDecimals: true }, constraints:{ precision:13, scale:2 }}">
						<m:customData>
							<core:CustomData key="title" value="{modelVtaMov>Group}" writeToDom="true"/>
						</m:customData>
					</m:Text>
				</template>
			</Column>
			<Column width="7.4rem" headerSpan="2,1">
				<multiLabels>
					<m:Label text="Retiradas" textAlign="Center" width="100%"/>
					<m:Label text="Nº. Oper." textAlign="Center" width="100%"/>
				</multiLabels>
				<template>
